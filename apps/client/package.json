{
  "name": "@cartop/client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ./environments/.env.development --no-override env-cmd -f ./environments/.env.local --silent next dev --turbopack --port=3001",
    "dev:ssl": "yarn dev --experimental-https",
    "build": "env-cmd -f \"$ENV_FILE\" next build",
    "build:dev": "ENV_FILE=./environments/.env.development yarn build",
    "build:staging": "ENV_FILE=./environments/.env.staging yarn build",
    "build:test": "ENV_FILE=./environments/.env.test NODE_ENV=test yarn build",
    "dev:turbo": "next dev --turbopack",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write \"src/**/*.{ts,tsx,js,css,json}\"",
    "fix": "run-p -lsc \"fix:*\"",
    "lint:eslint": "eslint src --max-warnings=0",
    "lint:prettier": "prettier \"src/**/*.{ts,tsx,js,css,json}\" -c",
    "lint:tsc": "tsc",
    "lint": "yarn lint:eslint",
    "start": "next start",
    "storybook:build": "storybook build",
    "storybook:start": "http-server storybook-static -p 6006 -c-1",
    "storybook:test-ci": "yarn concurrently -k -s first -n \"storybook,test\" -c \"magenta,blue\" \"yarn storybook:start --silent\" \"yarn wait-on tcp:127.0.0.1:6006 && yarn storybook:test\"",
    "storybook:test-docker": "cd ../../scripts/storybook-test-runner && ./docker-test-runner.sh crm",
    "storybook:test-update": "yarn test-storybook --updateSnapshot",
    "storybook:test": "yarn test-storybook",
    "storybook": "storybook dev -p 6006",
    "test": "TZ=GMT jest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@cartop/api-client": "workspace:*",
    "@cartop/ui-utils": "workspace:*",
    "@cartop/validation": "workspace:*",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/client-preset": "^5.1.0",
    "@hookform/resolvers": "^3.9.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@tanstack/react-query": "^5.90.2",
    "eslint-config-cartop": "link:../../packages/eslint-config-cartop",
    "gsap": "^3.13.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "postcss": "^8.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.54.2",
    "swiper": "^12.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/cli": "^4.1.14",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.6.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "concurrently": "^9.2.1",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.3",
    "env-cmd": "^11.0.0",
    "eslint": "^9.20.1",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.7.9",
    "eslint-plugin-boundaries": "^5.0.2",
    "eslint-plugin-no-secrets": "^2.2.1",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-storybook": "9.1.10",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "eslint-plugin-unicorn": "^61.0.2",
    "graphql": "^16.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.4",
    "wait-on": "^9.0.1"
  },
  "resolutions": {
    "farmhash": "^3.3.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/node_modules/**"
    ],
    "excludeAfterRemap": true,
    "report-dir": "coverage-cypress"
  }
}
